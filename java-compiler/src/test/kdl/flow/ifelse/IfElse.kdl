type IfElse {
  const GOOD : "hello from IfElse"
  const FAIL : "This message should not have appeared"

  main {
    if true {
      println(GOOD)
    }
    else {
      println(FAIL)
    }

    if false {
      println(FAIL)
    }
    else {
      println(GOOD)
    }
  }
}
